{
	"name": "mapcn-svelte",
	"private": true,
	"version": "0.2.0",
	"type": "module",
	"packageManager": "pnpm@10.28.1",
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm build:registry && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
